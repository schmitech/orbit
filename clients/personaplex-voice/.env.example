# ORBIT PersonaPlex Voice Client Configuration
# Copy this file to .env.local and update with your values

# ORBIT Server WebSocket URL (without /ws/voice path)
VITE_ORBIT_SERVER_URL=ws://localhost:3000

# API Key for the adapter (required)
VITE_API_KEY=personaplex

# Application title
VITE_APP_TITLE=ORBIT PersonaPlex Voice

VITE_DISPLAY_SETTINGS=false
